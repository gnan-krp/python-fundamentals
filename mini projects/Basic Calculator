def add(x, y):
    return x + y

def subtract(x, y):
    return x - y

def multiply(x,y):
    return x*y

def divide(x,y):
    return x/y

def exponent(x,y):
    return x**y

print("Sum:", add(5, 3))
print("Difference:", subtract(5, 3))
print("Multiplication:",multiply(5,3))
print("Division:",divide(5,3))
print(Exponential:",exponent(5,3))
